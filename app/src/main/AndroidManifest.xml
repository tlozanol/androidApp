<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.androidlabs">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.tlozano.androidlabs2.StartActivity" />
        <!--
        <activity android:name=".LoginActivity">

        </activity>
        -->
        <!-- <activity android:name=".ListItemsActivity" /> -->
        <activity android:name="com.example.tlozano.androidlabs2.ListItemsActivity" />
        <activity android:name="com.example.tlozano.androidlabs2.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.tlozano.androidlabs2.ChatWindow" />
        <activity android:name="com.example.tlozano.androidlabs2.WeatherForecast" />
    </application>

</manifest>